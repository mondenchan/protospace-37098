<div class="card">
  <%#= link_to image_tag("プロトタイプの画像", class: :card__img ), root_path%>
  <%= link_to image_tag(prototype.image, class: "card__img" ), prototype_path(prototype.id) %>
  <div class="card__body">
    <%#= link_to "プロトタイプのタイトル", root_path, class: :card__title%>
    <%= link_to "#{prototype.title}のタイトル", prototype_path(prototype.id), class: :card__title%>
    
    <p class="card__summary">
      <%#= "プロトタイプのキャッチコピー" %>
      <%= "#{prototype.catch_copy} "%>

    </p>
    <%= link_to "by #{prototype.user.name}", user_path(prototype.user), class: :card__user %>
    <%#= link_to "by #{prototype.user.name}", prototype_path, class: :card__user %>
    <%#= link_to "by #{prototype.user.name}", prototype_path(prototype.user), class: :card__user %>
    <%#= jeffのを参照元は、下のもの%>
    <%#= link_to "by #{prototype.user.name}", prototype_path(prototype.id), class: :card__user %>
  </div>
</div>

